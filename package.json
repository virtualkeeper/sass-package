{
    "name": "sass-nugget",
    "version": "1.1.0",
    "description": "The sass package is for the visual layer.",
    "repository": "https://github.com/virtualkeeper/sass-package",
    "main": "index.js",
    "dependencies": {
        "autoprefixer": "^10.2.6",
        "gulp-babel": "^8.0.0",
        "gulp-concat": "^2.6.1",
        "gulp-imagemin": "^8.0.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^5.0.0",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-uglify": "^3.0.2",
        "postcss-cli": "^8.3.1"
    },
    "devDependencies": {
        "@popperjs/core": "^2.9.2",
        "bootstrap": "^5.0.1",
        "browserlist": "^1.0.1",
        "clean-css": "^5.1.2",
        "concat": "^1.0.3",
        "delete": "^1.1.0",
        "gulp": "^4.0.2",
        "gulp-postcss": "^9.0.0",
        "postcss": "^8.3.5",
        "sass": "^1.44.0",
        "sass-loader": "^12.0.0",
        "stylelint": "^13.13.1",
        "stylelint-config-standard": "^22.0.0",
        "webpack": "^5.38.1"
    },
    "scripts": {
        "dev": "sass --quiet --watch sass/style.scss:css/style.css sass/components/:css/components",
        "init":  "sass --quiet --style=compressed --no-source-map sass/style.scss:sass/temp/standard/style.css sass/components/:sass/temp/standard/components",
        "prefix": "postcss sass/temp/standard/* --use autoprefixer -d css/",
        "prod": "init && prefix",
        "clean": "gulp clean --force --quiet"
    },
    "browserslist": "last 4 versions",
    "author": "Darrin Ezell",
    "license": "ISC"
}
